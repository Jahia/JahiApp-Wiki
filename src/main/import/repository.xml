<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:sv="http://www.jcp.org/jcr/sv/1.0"
         xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
         xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <wiki j:siteType="module" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite"
              changefreq="monthly" j:installedModules="wiki" priority="0.5">
            <home jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" changefreq="monthly"
                  j:templateNode="/templateSets/wiki/templates/base/wiki" priority="0.5"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <base jcr:mixinTypes="jmix:renderable jmix:theme" jcr:primaryType="jnt:template" j:view="apps">
                    <wiki jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <tabs jcr:primaryType="jnt:tabularList">
                                <last-pages jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="Latest Wiki Pages" jcr:primaryType="jnt:translation"/>
                                    <contentRetrieval j:criteria="jcr:lastModified" maxItems="20"
                                                      jcr:mixinTypes="jmix:renderable" j:type="jnt:wikiPage"
                                                      jcr:primaryType="jnt:contentRetrieval" j:view="linkList"
                                                      j:sortDirection="asc"/>
                                </last-pages>
                                <create-new-page jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="Create new page" jcr:primaryType="jnt:translation"/>
                                    <create-a-wiki-page jcr:primaryType="jnt:wikiPageFormCreation">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="Create a wiki page"
                                                          jcr:primaryType="jnt:translation"/>
                                    </create-a-wiki-page>
                                </create-new-page>
                            </tabs>
                        </pagecontent>
                    </wiki>
                    <wiki-content jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:wikiPage"
                                  j:defaultTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="2col124" jcr:primaryType="jnt:row"/>
                        </pagecontent>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <tabularList jcr:primaryType="jnt:tabularList">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Content"
                                                      jcr:primaryType="jnt:translation"/>
                                    <mainResourceDisplay j:mainResourceView="default"
                                                         jcr:primaryType="jnt:mainResourceDisplay"/>
                                </pagecontent>
                                <contribute jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="Contribute" jcr:primaryType="jnt:translation"/>
                                    <mainResourceDisplay j:mainResourceView="contribute"
                                                         jcr:primaryType="jnt:mainResourceDisplay"/>
                                </contribute>
                                <history jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="History"
                                                      jcr:primaryType="jnt:translation"/>
                                    <history j:mainResourceView="history"
                                                         jcr:primaryType="jnt:mainResourceDisplay"/>
                                    <compare j:mainResourceView="compare"
                                                           jcr:primaryType="jnt:mainResourceDisplay"/>
                                </history>
                            </tabularList>
                        </row-col1>
                        <rol-col2 jcr:primaryType="jnt:contentList"/>
                    </wiki-content>
                </base>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <files jcr:primaryType="jnt:folder"/>
            </templates>
        </wiki>
    </templateSets>
</content>


